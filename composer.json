{
    "name": "rafni/validations",
    "description": "Validator module for Laravel Validator 5.5",
    "license": "MIT",    
    "authors": [
        {
            "name": "Rafni",
            "email": "rafnicoder@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Rafni\\Validations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rafni\\Validations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rafni\\Validations\\ValidationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
